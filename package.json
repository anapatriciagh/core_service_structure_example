{
  "name": "service_service",
  "version": "1.0.0",
  "description": "Service module for Obapet project. Powered with Node.js.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon dist/server.js",
    "build": "npx babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "tsnd src/server.ts --respawn --transpile-only --ignore-watch node_modules",
    "test": "jest",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration-generate": "npm run typeorm:cli --migration:generate -n",
    "debug": "nodemon --watch 'src/**' --exec 'ts-node src/server.ts' -e ts",
    "nodemon": "nodemon src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
