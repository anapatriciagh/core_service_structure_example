    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status": "^1.3.2",
    "jest": "^24.9.0",
    "jwt-decode": "^3.1.2",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^6.2.0",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32"
  },
